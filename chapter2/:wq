
set number
filetype on
set history=1000
set background=dark
syntax on
set autoindent
set smartindent
set tabstop=4
set shiftwidth=4
set showmatch
set guioptions-=T
set vb t_vb=
set ruler
set nohls
set incsearch
if has('clipboard')
set clipboard=unnamed,unnamedplus
endif
if has("vms")
set nobackup
else
set backup
endif
call plug#begin('~/.vim/plugged')
Plug 'ron89/thesaurus_query.vim'
Plug 'ervandew/supertab'
Plug 'davidhalter/jedi-vim'
Plug 'scrooloose/nerdtree'
call plug#end()
